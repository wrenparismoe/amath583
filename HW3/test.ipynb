    "for (int n = 2; n < 1025; n++) {\n",
    "    // Check if n is a power of 2\n",
    "    power2 = ((n & (n - 1)) == 0);\n",
    "    if (power2) {\n",
    "        std::cout << n << \": \" << power2 << std::endl;\n",
    "    }\n",
